#### Create datasource
curl -XPOST -i \
	--cookie "$COOKIE" \
	-H "Authorization: Bearer $API_TOKEN" \
	-H "Content-Type: application/json" \
	--data-binary @/data/career/cern/repos/cloudbank-monit-private/grafana/influxDB_datasource.json \
	$HOST/api/datasources

#### Create GCP dashboard
curl -XPOST -i \
	--cookie "$COOKIE" \
	-H "Authorization: Bearer $API_TOKEN" \
	-H "Content-Type: application/json" \
	--data-binary @/data/career/cern/repos/cloudbank-monit-private/grafana/gcp/gcp_projects_daily.json \
	$HOST/api/dashboards/db

#### Create AWS dashboard
curl -XPOST -i \
	--cookie "$COOKIE" \
	-H "Authorization: Bearer $API_TOKEN" \
	-H "Content-Type: application/json" \
	--data-binary @/data/career/cern/repos/cloudbank-monit-private/grafana/aws/aws_projects_daily.json \
	$HOST/api/dashboards/db
